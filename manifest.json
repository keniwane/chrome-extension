{
    "manifest_version": 3,
    "name": "JSON.derulo",
    "version": "1.0",
    "description": "Displays a summary of the ToS and its rating.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "Icons/Sid.jpg"
        }
    }
}